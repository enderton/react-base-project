{
  "name": "react-base-project",
  "version": "0.0.1",
  "description": "Starter Base React Project using, Webpack, Babel, LESS, etc.",
  "main": "server/server.js",
  "scripts": {
    "build": "webpack",
    "start": "NODE_ENV='production' webpack && ./node_modules/.bin/babel server/server.js -o ./dist/server.js && node dist/server.js",
    "dev-start": "NODE_DEV='dev' webpack-dev-server --content-base client/ --progress --inline --hot"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enderton/react-base-project.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "starter"
  ],
  "author": "Maria Enderton",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "node-sass": "^4.9.3",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.21.0",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "express": "^4.14.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}
