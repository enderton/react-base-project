{
  "name": "react-base-project",
  "version": "0.0.1",
  "description": "Starter Base React Project using, Webpack, Babel, LESS, etc.",
  "main": "server/server.js",
  "scripts": {
    "build": "webpack",
    "start": "NODE_ENV='production' webpack && ./node_modules/.bin/babel server/server.js -o ./dist/server.js && node dist/server.js",
    "dev-start": "webpack-dev-server --content-base client/ --progress --inline --hot"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enderton/react-base-project.git"
  },
  "keywords": [
    "react",
    "webpack",
    "babel",
    "starter"
  ],
  "author": "Maria Enderton",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.13.1",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "react-transform-hmr": "^1.0.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "express": "^4.13.3",
    "react": "^0.14.5"
  }
}
